<h1>Monsters#create</h1>
<p>Find me in app/views/monsters/create.html.erb</p>

<%= simple_form_for @monster do |f| %>
  <%= f.input :name, label: "nom" %>
  <%= f.input :specie, label: "espece" %>
  <%= f.input :description, label: "description"%>
  <%= f.input :price, label: "prix"%>
  <%= f.input :image, :as => "image_tag" %>
  <%= f.button :submit %>
<% end %>
