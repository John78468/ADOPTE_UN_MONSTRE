<h1>Monsters#new</h1>
<p>Find me in app/views/monsters/new.html.erb</p>
<%= simple_form_for @list do |f| %>
  <%= f.input :name, label: "nom" %>
  <%= f.input :specie, label: "espece" %>
  <%= f.input :description, label: "description"%>
  <%= f.input :price, label: "prix"%>
  <%= f.input :image, :as => "image_tag" %>
  <%= f.button :submit %>
<% end %>
